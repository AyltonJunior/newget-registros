{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "web-dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/web-dashboard/(.*)",
      "dest": "/web-dashboard/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 